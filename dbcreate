
CREATE TABLE u1hybrid(id INT AUTO_INCREMENT, sites INT, lattice VARCHAR(50), NS INT, N0 INT, t1 DOUBLE, t2 DOUBLE, h DOUBLE, eta DOUBLE, txtdsc VARCHAR(50), P1 DOUBLE, dP1 DOUBLE, P2 DOUBLE, dP2 DOUBLE, muz DOUBLE, ap INT(1), td1 DOUBLE, td2 DOUBLE, r2 DOUBLE, ts TIMESTAMP DEFAULT CURRENT_TIMESTAMP, PRIMARY KEY(id));

CREATE TABLE paired(id INT AUTO_INCREMENT, sites INT, lattice VARCHAR(50), txtdsc VARCHAR(50), NS INT, N0 INT, ap INT(1), mu DOUBLE, t1 DOUBLE, t2 DOUBLE, dd DOUBLE, lth DOUBLE, lr1 DOUBLE, lr2 DOUBLE, P1 DOUBLE, dP1 DOUBLE, P2 DOUBLE, dP2 DOUBLE, R4 DOUBLE, dR4 DOUBLE, ts TIMESTAMP DEFAULT CURRENT_TIMESTAMP, PRIMARY KEY(id));

CREATE TABLE liquid(id INT AUTO_INCREMENT, sites INT, lattice VARCHAR(50), txtdsc VARCHAR(50), NS INT, N0 INT, apx INT(1), apy INT(1), TR INT(1), gR int(1), l3 DOUBLE, l2 DOUBLE, l1 DOUBLE, xi1 DOUBLE, xi2 DOUBLE, xi3 DOUBLE, dd1 DOUBLE, dd2 DOUBLE, dd3 DOUBLE, a1 DOUBLE, a2 DOUBLE, a3 DOUBLE, b1 DOUBLE, b2 DOUBLE, b3 DOUBLE, P1 DOUBLE, dP1 DOUBLE, P2 DOUBLE, dP2 DOUBLE, P3 DOUBLE, dP3 DOUBLE, ts TIMESTAMP DEFAULT CURRENT_TIMESTAMP, mc_length INT, nbin INT, PRIMARY KEY(id));

